{
    "name": "maciel-scss",
    "version": "1.0.0",
    "description": "login style scss",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "scss": "node-sass --watch scss -o css --output-style compressed",
        "scss-n": "node-sass --watch scss -o css --output-style nested",
        "scss-e": "node-sass --watch scss -o css --output-style expanded ",
        "scss-p": "node-sass --watch scss -o css --output-style compact",
        "scss-c": "node-sass --watch scss -o css --output-style compressed"
    },
    "keywords": [
        "scss",
        "login"
    ],
    "author": "rickymaciel",
    "license": "MIT",
    "dependencies": {
        "node-sass": "^4.13.1"
    }
}
